# mount point	fstype		device				options (length for cryptfs encryption footer)
/recovery	mtd		recovery
/boot		mtd		boot
/radio		mtd		radio
/data		ext4		/dev/lvpool/userdata
/cache		yaffs2		cache
/datadata	yaffs2		datadata
/system		ext4		/dev/lvpool/system
/emmc		vfat		/dev/block/mmcblk1p1	flags=display="Micro SDCard";storage;wipeingui;removable

# raw partitions
/raw_sd		vfat		/dev/block/mmcblk0p1	flags=wipeingui;removable
/raw_data	ext4		/dev/block/mmcblk0p2	flags=wipeingui;removable

